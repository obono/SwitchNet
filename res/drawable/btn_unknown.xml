<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

<item android:state_pressed="false" android:state_focused="false">
    <shape android:shape="rectangle">
        <gradient
            android:angle="135"
            android:startColor="#884400"
            android:endColor="#FFCC88"
            android:type="linear"
            />
        <corners android:radius="16dip" />
        <stroke android:width="4dip" android:color="#442200" />
    </shape>
</item>

<item android:state_pressed="true" android:state_focused="false">
    <shape android:shape="rectangle">
        <gradient
            android:angle="135"
            android:startColor="#FFCC88" 
            android:endColor="#884400"
            android:type="linear"
            />
        <corners android:radius="16dip" />
        <stroke android:width="4dip" android:color="#AA8866" />
    </shape>
</item>

</selector>